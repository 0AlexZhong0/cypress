{
  "name": "cypress",
  "version": "0.2.2",
  "description": "Javascript Test Engine",
  "main": "app://app/nw/public/index.html",
  "single-instance": false,
  "window": {
    "frame": false,
    "toolbar": false,
    "show": false,
    "height": 400,
    "width": 300
  },
  "scripts": {
    "snapshot": "gulp nw:snapshot",
    "build": "gulp client:build nw:build",
    "watch": "gulp client nw",
    "deploy": "gulp deploy",
    "test-server": "NODE_ENV=test BLUEBIRD_DEBUG=1 mocha --opts spec/server/mocha.opts --watch",
    "test-server-debug": "NODE_ENV=test BLUEBIRD_DEBUG=1 node-debug --hidden=node_modules --no-preload _mocha --opts spec/server/mocha.opts --watch",
    "test-server-ci": "NODE_ENV=test BLUEBIRD_DEBUG=1 mocha --opts spec/server/mocha.opts"
  },
