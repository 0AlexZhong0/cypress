{
  "name": "Cypress",
  "version": "0.14.3",
  "description": "Javascript Test Engine",
  "main": "index.js",
  "single-instance": false,
  "scripts": {
    "start": "node index.js",
    "server": "node index.js --mode server",
    "build": "gulp client:build",
    "watch": "gulp client",
    "deploy": "gulp deploy",
    "release": "gulp release",
    "test": "npm run test-server & chokidar 'spec/server/**/*' 'lib/**/*' -c 'npm run test-server' --polling --poll-interval=250 ",
    "test-cov": "NODE_COVERAGE=true NODE_ENV=test CYPRESS_ENV=test BLUEBIRD_DEBUG=1 istanbul cover node_modules/.bin/_mocha -- --opts ./spec/server/mocha.opts",
    "test-cov-process": "NODE_COVERAGE=true NODE_ENV=test CYPRESS_ENV=test BLUEBIRD_DEBUG=1 istanbul cover --include-pid",
    "test-server": "NODE_ENV=test CYPRESS_ENV=test BLUEBIRD_DEBUG=1 DEBUG=nock.*,-nock.common,socket.io:* mocha --opts spec/server/mocha.opts",
    "test-server-debug": "NODE_ENV=test CYPRESS_ENV=test BLUEBIRD_DEBUG=1 DEBUG=nock.*,-nock.common,socket.io:* node-debug --hidden='(node_modules|bower_components)' --no-preload _mocha --opts spec/server/mocha.opts --watch",
    "codecov": "codecov",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress.git"
  },
