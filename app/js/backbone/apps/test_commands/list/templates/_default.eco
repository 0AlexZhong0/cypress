<div class="command-wrapper">
  <span class="command-number">
    <span><%= @number %></span>
  </span>
  <span class="command-method">
    <span><%= @name %></span>
  </span>
  <span class="command-message">
    <% if @alias: %>
      <span class="command-alias <%= @aliasType %>" title="Found an alias for: '<%= @alias %>'">@<%= @alias %></span>
    <% else: %>
      <span><%= @message %></span>
    <% end %>
  </span>
  <% if @shouldDisplayControls: %>
    <span class="command-controls">
      <% if @aliased: %>
        <span class="command-alias" title="<%= @message %> aliased as: '<%= @aliased %>'">
          <%= @aliased %>
        </span>
      <% end %>
      <!-- <i class="fa fa-pause"></i> -->
      <i class="fa fa-eye-slash shown"></i>
      <label class="label label-warning"><%= @numRetries %></label>
      <% if @numElements > 1: %>
        <label title="<%= @numElements %> matched elements" class="label label-default"><%= @numElements %></label>
      <% end %>
      <i class="fa fa-search fa-flip-horizontal"></i>
    </span>
  <% end %>
</div>