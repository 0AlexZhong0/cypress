<div class="command-wrapper">
  <span class="command-number">
    <%= @numberFormatted %>
  </span>
  <span class="command-method"><%= @method %></span>
  <% if @sequence: %>
    <span class="command-sequence"><%= @sequence %></span>
  <% else if @selector: %>
    <span class="command-selector"><%= @selector %></span>
  <% end %>
  <% if @length > 1: %>
    <label title="<%= @length %> matched elements" class="label label-primary"><%= @length %></label>
  <% end %>
  <% if @error: %>
    <span class="command-error"><%= @error %></span>
  <% end %>
  <% if @shouldDisplayControls: %>
    <span class="command-controls">
      <!-- <i class="fa fa-pause"></i> -->
      <i class="fa fa-search fa-flip-horizontal"></i>
    </span>
  <% end %>
</div>