<td>
  <%= @index %>.
</td>
<td>
  <div class="job-state-container">
    <i class="<%= @stateIcon %>"></i>
    <span class="job-state"><%= @stateFormatted %></span>
  </div>
</td>
<td>
  <div class="image-wrapper">
    <img src="/eclectus/img/<%= @browser %>_32x32.png" />
  </div>
  <%= @browser %> <%= @version %>
</td>
<td>
  <i class="fa fa-<%= @icon %>"></i>
  <%= @os %>
</td>
<td>
  <% if @id: %>
    <a href="<%= @url %>" target="_blank">
      <i class="fa fa-crosshairs"></i>
      Manual
    </a>
    |
    <a href="http://saucelabs.com/tests/<%= @id %>" target="_blank">
      <i class="fa fa-external-link"></i>
      Details
    </a>
  <% end %>
</td>
<td>
  <% if @time: %>
    <i class="fa fa-clock-o"></i>
    <%= @timeFormatted %> secs
  <% end %>
</td>