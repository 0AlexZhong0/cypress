<div id="projects">
  <div class="alert alert-danger error">
    <p>
      <i class="fa fa-warning"></i>
      <strong>Project can't be added</strong>
    </p>
    <p class="bg-danger">
      <%- @message.replace("\n", "<br /><br />") %>
    </p>
    <button data-ok class="btn btn-default btn-sm">
      Dismiss
    </button>
  </div>
</div>