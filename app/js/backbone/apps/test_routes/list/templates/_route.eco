<td><%= @method %></td>
<td>
  <%= @urlFormatted %>
</td>
<td><%= @status %></td>
<td>
  <% if @alias: %>
    <span class="route-alias" title="Aliased this route as: <%= @alias %>">
      <%= @alias %>
    </span>
  <% end %>
</td>
<td>
  <% if @numResponses is 0: %>
  <span class="text-fail">
    -
  </span>
  <% else: %>
    <%= @numResponses %>
    <% end %>
</td>