<% if @paused: %>
  <div id="pause-controls" class="btn-group btn-group-lg" role="group">
<!--     <button type="button" class="btn btn-default active disabled">
      <i class="fa fa-pause red"></i>
    </button> -->
    <button data-js-resume type="button" class="btn btn-default" title="Play Commands">
      <i class="fa fa-play"></i>
    </button>
    <% if @nextCmd: %>
      <button data-js-next type="button" class="btn btn-default" title="Next Command: <%= @nextCmd %>">
        <i class="fa fa-step-forward"></i>
      </button>
    <% end %>
  </div>
<% end %>


