<ul id="stats-container">
  <li id="tests-passed">
    <i class="fa fa-check"></i>
    <span class="num">
      <%= @count(@passed) %>
    </span>
  </li>
  <li id="tests-failed">
    <i class="fa fa-times"></i>
    <span class="num">
      <%= @count(@failed) %>
    </span>
  </li>
  <li id="tests-pending">
    <i class="fa fa-circle-o-notch"></i>
    <span class="num">
      <%= @count(@pending) %>
    </span>
  </li>
  <li id="tests-duration">
    <span class="num">
      <%= @count(@duration) %>
    </span>
  </li>
</ul>