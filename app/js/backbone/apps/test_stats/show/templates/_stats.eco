<ul id="stats-container">
  <li id="tests-passed">
    <i class="fa fa-check"></i>
    <span class="num"><%= @count(@passed) %></span>
    <span>Passed</span>
  </li>
  <li id="tests-failed">
    <i class="fa fa-times"></i>
    <span class="num"><%= @count(@failed) %></span>
    <span>Failed</span>
  </li>
  <li id="tests-pending">
    <i class="fa fa-circle-o-notch"></i>
    <span class="num"><%= @count(@pending) %></span>
    <span>Pending</span>
  </li>
  <li id="tests-duration">
    <span>Runtime:</span>
    <span class="num"><%= @count(@duration) %></span>
    <span>secs</span>
  </li>
</ul>